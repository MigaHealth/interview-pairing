{
  "name": "@migahealth/interview-pairing",
  "version": "0.0.1",
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "start": "docker compose up --build -d -V && yarn migrate:dev && yarn db:seed",
    "migrate:dev": "DB_HOST=localhost yarn prisma migrate dev && yarn generate:app",
    "generate:app": "docker compose exec app yarn prisma generate",
    "db:seed": "docker compose exec app yarn prisma db seed",
    "test": "vitest"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@mantine/core": "^6.0.11",
    "@mantine/form": "^6.0.11",
    "@mantine/hooks": "^6.0.11",
    "@prisma/client": "^4.14.1",
    "@tabler/icons-react": "^2.20.0",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.28.1",
    "@trpc/next": "^10.28.1",
    "@trpc/react-query": "^10.28.1",
    "@trpc/server": "^10.28.1",
    "@vitejs/plugin-react": "^4.0.0",
    "next": "^13.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prisma": "^4.14.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "^4.1.2",
    "vitest": "^0.28.5"
  }
}
